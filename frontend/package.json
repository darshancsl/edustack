    "lint:fix": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --fix",
    "lint-staged": "lint-staged",
    "format": "npm run lint:fix",
    "prettier:fix": "prettier --write",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
